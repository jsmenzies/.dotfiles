# This is Git's per-user configuration file.
[user]
	name = James Menzies
	email = james@menzies.io
[init]
	defaultBranch = main
[core]
	autocrlf = input
[color]
	ui = true


[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[submodule]
	recurse = true
